{
  "$schema": "http://json.schemastore.org/template",
  "author": "Homely's groovin' developers!",
  "classifications": [ "Web", "WebAPI" ],
  "identity": "Homely.AspNetCore.WebApi.Template",
  "name": "Homely ASP.NET Core WebApi Template",
  "shortName": "homelyapi",
  "tags": {
    "language": "C#"
  },
  "sourceName": "Homely.AspNetCore.WebApi.Template",
  "preferNameDirectory" : true,
  "symbols":{
    "swagger-title": {
        "type": "parameter",
        "defaultValue": "My API",
        "replaces":"swagger-title"
    },
    "swagger-version": {
        "type": "parameter",
        "defaultValue": "v1",
        "replaces":"swagger-version"
    },
    "swagger-routeprefix": {
      "type": "parameter",
      "defaultValue": "swagger",
      "replaces":"swagger-routeprefix"
    }
  }
}